{
    "name": "irishdan/notification-bundle",
    "description": "A notification bundle for the Symfony framework",
    "license": "MIT",
    "homepage": "https://danbyrne.me/notification-bundle",
    "authors": [
        {
            "name": "Daniel Byrne",
            "email": "danielbyrne@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/framework-bundle": "4.1.0 ",
        "symfony/security": "^4.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "doctrine/doctrine-bundle": "^1.9.1",
        "doctrine/orm": "^2.6.1",
        "symfony/monolog-bundle": "^3.3.0",
        "symfony/swiftmailer-bundle": "^3.2.2",
        "symfony/twig-bundle": "^4.1.0",
        "pusher/pusher-php-server": "^3.2.0"
    },
    "autoload": {
        "psr-4": {
            "IrishDan\\NotificationBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
