{
    "name": "irishdan/notifications-bundle",
    "description": "A notifications bundle for symfony framework",
    "license": "MIT",
    "homepage": "https://nomadapi.io",
    "support": {
        "issues": "https://nomadapi.io/issues",
        "source": "https://nomadapi.io/framework"
    },
    "authors": [
        {
            "name": "Daniel Byrne",
            "email": "danielbyrne@outlook.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "symfony/framework-bundle": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Irishdan\\NotificationsBundle\\": ""
        }
    },
    "suggest": {
        "guzzlehttp/guzzle": "Required to use the Slack transport.",
        "nexmo/client": "Required to use the Nexmo transport."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
